{
  "name": "react-redux-sandbox",
  "version": "0.1.0",
  "description": "sandbox for react and redux",
  "scripts": {
    "clean": "rimraf output",
    "lint": "eslint .",
    "test:node": "mocha",
    "cover:node": "nyc --nycrc-path=test/nyc.conf.js node_modules/mocha/bin/_mocha",
    "test:chrome": "karma start test/karma.conf.js --browsers Chrome --single-run",
    "test:debug": "karma start test/karma.conf.js --browsers Firefox",
    "build": "rimraf output/build && node tools/build && webpack --hide-modules --config tools/webpack.config.js",
    "build:test": "webpack --hide-modules --config test/webpack.test.config.js",
    "test": "npm run lint && npm run cover:node && npm run build && npm run build:test && npm run test:chrome",
    "test:travis": "npm run lint && npm run cover:node && npm run build",
    "watch": "rimraf output/build && node tools/build && webpack --hide-modules --config tools/webpack.dev.config.js && webpack-dev-server --config tools/webpack.dev.config.js",
    "coveralls": "coveralls < output/coverage/lcov.info",
    "travis": "npm run test:travis && npm run coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/larsthorup/react-redux-sandbox.git"
  },
  "author": "Lars Thorup <lars@zealake.com> (http://github.com/larsthorup)",
  "license": "MIT",
  "homepage": "https://github.com/larsthorup/react-redux-sandbox#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "fetch-mock": "^7.7.3",
    "fs-extra": "^9.0.1",
    "html-webpack-plugin": "^4.3.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^2.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.12.21",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "semistandard": "^14.2.0",
    "sinon": "^9.0.2",
    "skin-deep": "^1.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "camelcase": "^5.3.1",
    "css-loader": "^3.5.3",
    "prop-types": "^15.7.2",
    "purecss": "^1.0.1",
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^3.2.6",
    "redux": "^4.0.5",
    "redux-thunk": "^2.0.1",
    "style-loader": "^1.2.1"
  }
}
